	<xs:complexType name="dashboardRequestBody">
		<xs:complexContent>
			<xs:extension base="abstractRequestBody">
				<xs:all>
					<xs:element ref="dashboardInfo" minOccurs="0" />
					<xs:element name="data" type="data" minOccurs="0" />
					<xs:element name="filters" type="filters" minOccurs="0" />
					<xs:element name="widgets" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="widget" minOccurs="1" maxOccurs="unbounded" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="abstractRequestBody" abstract="true">
		<xs:sequence />
	</xs:complexType>

	<xs:complexType name="dashboardInfo">
		<xs:all>
			<xs:element ref="filters" minOccurs="0" />
			<xs:element name="template" type="xs:string" minOccurs="0" />
			<xs:element name="title" type="title" minOccurs="0" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="filters">
		<xs:sequence>
			<xs:element ref="filter" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="type" type="filtersType" />
		<xs:attribute name="template" type="xs:string" />
		<xs:attribute name="cssClass" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="filterDTO">
		<xs:all>
			<xs:element ref="data" minOccurs="1" maxOccurs="1" />
			<xs:element name="style" type="htmlCSS" minOccurs="0"
				maxOccurs="1" />
			<xs:element name="validation" type="validation" minOccurs="0"
				maxOccurs="1" />
		</xs:all>
		<xs:attribute name="event" type="filterEvents" />
		<xs:attribute name="id" type="nonEmptyString" use="required" />
		<xs:attribute name="initIndexes" type="xs:string" />
		<xs:attribute name="parent" type="xs:string" />
		<xs:attribute name="state" type="xs:string" />
		<xs:attribute name="template" type="xs:string" />
		<xs:attribute name="title" type="xs:string" />
		<xs:attribute name="type" type="filterUnion" use="required" />
		<xs:attribute name="cssClass" type="xs:string" />
		<xs:attribute name="maxSelect" type="maxSelectAllowType" />
	</xs:complexType>

	<xs:complexType name="validation">
		<xs:attribute name="maxLength" type="xs:long" />
		<xs:attribute name="minLength" type="xs:long" />
		<xs:attribute name="required" type="xs:boolean" />
		<xs:attribute name="validationType" type="validationType"
			use="required" />
	</xs:complexType>

	<xs:complexType name="data">
		<xs:sequence>
			<xs:element name="dataSource" type="dataSourceDef"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="analysis" type="analysisDef" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>

		<xs:attribute name="cacheConnectorName" type="xs:string"></xs:attribute>
		<xs:attribute name="cacheName" type="xs:string"></xs:attribute>
	</xs:complexType>

	<xs:complexType name="stream">
		<xs:sequence>
			<xs:element name="streamSource" type="streamSourceDef"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="analysisDef">
		<xs:sequence maxOccurs="1" minOccurs="1">
			<xs:element name="inputs" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="input" type="field" minOccurs="1"
							maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="outputs" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="output" type="output" minOccurs="0"
							maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="options" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="option" type="analysisOptions"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
<!-- 			<xs:any maxOccurs="1" minOccurs="0" namespace="##other"
				processContents="lax"></xs:any> -->
		</xs:sequence>
		<xs:attribute name="connector" type="xs:string" />
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="condition" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="analysisOptions">
		<xs:sequence />
		<xs:attribute name="key" type="xs:string" />
		<xs:attribute name="value" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="output">
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="value" type="xs:string" use="required" />
	</xs:complexType>

	<xs:complexType name="dataSourceDef">
		<xs:all>
			<xs:element name="options" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="option" type="queryOptions"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="queryParams" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="queryParam" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="queryType" minOccurs="1" />
		</xs:all>
		<xs:attribute name="connector" type="connectorTypeUnion"
			use="required" />
		<xs:attribute name="name" type="nonEmptyString" use="required" />
		<!-- <xs:attribute name="report" type="xs:string" /> -->
		<xs:attribute name="type" type="queryTypeEnum" default="find"></xs:attribute>
		<xs:attribute name="transaction" type="transactionTypeEnum"
			default="none"></xs:attribute>
		<xs:attribute name="timeout" type="xs:long" />
		<xs:attribute name="condition" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="streamSourceDef">
		<xs:all>
			<xs:element name="options" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="option" type="queryOptions"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="queryParams" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="queryParam" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="queryType" minOccurs="1" />
		</xs:all>
		<xs:attribute name="connector" type="connectorTypeUnion"
			use="required" />
		<xs:attribute name="name" type="nonEmptyString" use="required" />
	</xs:complexType>

	<xs:element name="queryType" abstract="true" />

	<xs:element name="query" substitutionGroup="queryType">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="queryBuilder" substitutionGroup="queryType">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="id" type="xs:string" use="required" />
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="queryTypeEnum">
		<xs:restriction base="xs:string">
			<xs:enumeration value="find" />
			<xs:enumeration value="insert" />
			<xs:enumeration value="delete" />
			<xs:enumeration value="update" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="transactionTypeEnum">
		<xs:restriction base="xs:string">
			<xs:enumeration value="none" />
			<xs:enumeration value="propogate" />
			<xs:enumeration value="isolated" />
		</xs:restriction>
	</xs:simpleType>


	<xs:complexType name="queryParam">
		<xs:sequence />
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="type" type="queryParamType" />
		<xs:attribute name="defaultValue" type="xs:string" />
		<xs:attribute name="isColumn" type="xs:boolean" />
	</xs:complexType>

	<xs:complexType name="queryOptions">
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="value" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="column">
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="columnKey" type="xs:string" />
		<xs:attribute name="displayOption" type="displayOptionType" />
	</xs:complexType>

	<xs:complexType name="vsTable">
		<xs:all>
			<xs:element name="columns" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="column" type="column" minOccurs="1"
							maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="plugins" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="plugin" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="plotOptions" type="tablePlotOptions"
				minOccurs="0"></xs:element>
		</xs:all>
		<xs:attribute name="type" type="tableType" default="datatable" />
	</xs:complexType>

	<xs:complexType name="tablePlotOptions">
		<xs:all>
			<xs:element name="datatable" type="dataTablePlotOptions"
				minOccurs="0"></xs:element>
			<xs:element name="pivot" type="pivotTablePlotOptions"
				minOccurs="0"></xs:element>
		</xs:all>
	</xs:complexType>

	<xs:complexType name="tableEvents">
		<xs:all>
			<xs:element name="rowCreated" minOccurs="0" maxOccurs="1"
				type="xs:string" />
			<xs:element name="headerCreated" type="xs:string"
				minOccurs="0" maxOccurs="1" />
			<xs:element name="tableCreated" type="xs:string"
				minOccurs="0" maxOccurs="1" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="plotOptionsColumn">
		<xs:attribute name="name" use="required"></xs:attribute>
		<xs:attribute name="searchable" type="xs:boolean" />
		<xs:attribute name="sortable" type="xs:boolean" />
		<xs:attribute name="color" type="xs:string" />
		<xs:attribute name="backgroundColor" type="xs:string" />
		<xs:attribute name="headerColor" type="xs:string" />
		<xs:attribute name="cellPaddingLeft" type="xs:string" />
		<xs:attribute name="cellPaddingRight" type="xs:string" />
		<xs:attribute name="cellPaddingTop" type="xs:string" />
		<xs:attribute name="cellPaddingBottom" type="xs:string" />
		<xs:attribute name="textAlign" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="dataTablePlotOptions">
		<xs:all>
			<xs:element ref="pagination" minOccurs="0"></xs:element>
			<xs:element name="columns" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="column" type="plotOptionsColumn"
							minOccurs="1" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="point" type="tablePoint" minOccurs="0"></xs:element>
			<xs:element name="events" type="tableEvents" minOccurs="0"></xs:element>
			<xs:element name="style" type="htmlCSS" minOccurs="0"></xs:element>
		</xs:all>
		<xs:attribute name="searchable" type="xs:boolean" />
		<xs:attribute name="sortable" type="xs:boolean" />
		<xs:attribute name="enableInfo" type="xs:boolean" />
	</xs:complexType>

	<xs:complexType name="pivotTablePlotOptions">
		<xs:attribute name="renderType" type="renderTypeEnum"
			default="Heatmap" />
		<xs:attribute name="aggregatorName" type="aggregatorType"
			default="count" />
		<xs:attribute name="showAxis" type="xs:boolean" default="false" />
	</xs:complexType>

	<xs:simpleType name="tableType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="datatable" />
			<xs:enumeration value="pivotTable" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="buttonType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="fullButton" />
			<xs:enumeration value="twoButton" />
			<xs:enumeration value="scrolling" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="property">
		<xs:attribute name="key" use="required"></xs:attribute>
		<xs:attribute name="value" use="required"></xs:attribute>
	</xs:complexType>

	<xs:complexType name="drillUpButton">
		<xs:all>
			<xs:element ref="position" minOccurs="0" />
			<xs:element ref="theme" minOccurs="0" />
		</xs:all>
		<xs:attribute name="relativeTo" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="dataClass">
		<xs:sequence />
		<xs:attribute name="color" type="xs:string" />
		<xs:attribute name="from" type="xs:double" />
		<xs:attribute name="name" type="xs:string" />
		<xs:attribute name="to" type="xs:double" />
	</xs:complexType>

	<xs:complexType name="mapNavigation">
		<xs:all>
			<xs:element name="buttonOptions" type="mapButtonOptions"
				minOccurs="0" />
			<xs:element name="buttons" type="mapButtons" minOccurs="0" />
		</xs:all>
		<xs:attribute name="enabled" type="xs:boolean" />
		<xs:attribute name="enableButtons" type="xs:boolean" />
		<xs:attribute name="enableDoubleClickZoom" type="xs:boolean" />
		<xs:attribute name="enableDoubleClickZoomTo" type="xs:boolean" />
		<xs:attribute name="enableMouseWheelZoom" type="xs:boolean" />
		<xs:attribute name="enableTouchZoom" type="xs:boolean" />
	</xs:complexType>

	<xs:complexType name="mapButtonOptions">
		<xs:all>
			<xs:element ref="theme" minOccurs="0" />
			<xs:element ref="style" minOccurs="0" />
		</xs:all>
		<xs:attribute name="align" type="alignType" />
		<xs:attribute name="height" type="xs:long" />
		<xs:attribute name="verticalAlign" type="verticalAlignType" />
		<xs:attribute name="width" type="xs:long" />
		<xs:attribute name="x" type="xs:long" />
	</xs:complexType>

	<xs:complexType name="mapButtons">
		<xs:all>
			<xs:element name="zoomIn" type="zoomIn" minOccurs="0" />
			<xs:element name="zoomOut" type="zoomOut" minOccurs="0" />
		</xs:all>
	</xs:complexType>

	<xs:complexType name="zoomIn">
		<xs:all>
			<xs:element ref="theme" minOccurs="0" />
			<xs:element ref="style" minOccurs="0" />
			<xs:element name="onclick" type="xs:string" minOccurs="0" />
		</xs:all>
		<xs:attribute name="align" type="alignType" />
		<xs:attribute name="height" type="xs:long" />

		<xs:attribute name="text" type="xs:string" />
		<xs:attribute name="verticalAlign" type="verticalAlignType" />
		<xs:attribute name="width" type="xs:long" />
		<xs:attribute name="x" type="xs:long" />
		<xs:attribute name="y" type="xs:long" />
	</xs:complexType>

	<xs:complexType name="zoomOut">
		<xs:all>
			<xs:element ref="theme" minOccurs="0" />
			<xs:element ref="style" minOccurs="0" />
			<xs:element name="onclick" type="xs:string" minOccurs="0" />
		</xs:all>
		<xs:attribute name="align" type="alignType" />
		<xs:attribute name="height" type="xs:long" />
		<xs:attribute name="text" type="xs:string" />
		<xs:attribute name="verticalAlign" type="verticalAlignType" />
		<xs:attribute name="width" type="xs:long" />
		<xs:attribute name="x" type="xs:long" />
		<xs:attribute name="y" type="xs:long" />
	</xs:complexType>

	<xs:complexType name="gradientStopValue">
		<xs:attribute name="gradientPosition" use="required"
			type="xs:float"></xs:attribute>
		<xs:attribute name="color" use="required" type="xs:string"></xs:attribute>
	</xs:complexType>

	<xs:complexType name="geoColorAxis">
		<xs:all>
			<xs:element name="dataClasses" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="dataClass" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="labels" minOccurs="0" />
			<xs:element name="events" type="axisEvents" minOccurs="0" />
			<xs:element name="gridMarker" type="marker" minOccurs="0" />
			<xs:element name="stops" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="stop" type="gradientStopValue"
							minOccurs="1" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="tickPositions" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="tickPosition" type="xs:long"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
		<xs:attribute name="tickPositioner" type="xs:string" />
		<xs:attribute name="tickWidth" type="xs:long" />
		<xs:attribute name="id" type="xs:string" />
		<xs:attribute name="dataClassColor" type="xs:string" />
		<xs:attribute name="endOnTick" type="xs:boolean" />
		<xs:attribute name="lineColor" type="xs:string" />
		<xs:attribute name="gridLineDashStyle" type="dashStyleType" />
		<xs:attribute name="gridLineWidth" type="xs:long" />
		<xs:attribute name="gridLineColor" type="xs:string" />

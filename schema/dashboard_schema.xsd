			<xs:enumeration value="series.name" />
			<xs:enumeration value="point.name" />
			<xs:enumeration value="point.category" />
			<xs:enumeration value="x" />
			<xs:enumeration value="y" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tooltipCustomeType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[^*+]+" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tooltipTypeUnion">
		<xs:union memberTypes="tooltipAttrType tooltipCustomeType" />
	</xs:simpleType>


	<xs:simpleType name="validationType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="alphabatic" />
			<xs:enumeration value="alphanumeric" />
			<xs:enumeration value="alphanumericWithQoutes" />
			<xs:enumeration value="alphaWithQoutes" />
			<xs:enumeration value="email" />
			<xs:enumeration value="integer" />
			<xs:enumeration value="numeric" />
			<xs:enumeration value="rawstring" />
			<xs:enumeration value="singlevalue" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="dashStyleType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Solid" />
			<xs:enumeration value="ShortDash" />
			<xs:enumeration value="ShortDot" />
			<xs:enumeration value="ShortDashDot" />
			<xs:enumeration value="ShortDashDotDot" />
			<xs:enumeration value="Dot" />
			<xs:enumeration value="Dash" />
			<xs:enumeration value="LongDash" />
			<xs:enumeration value="DashDot" />
			<xs:enumeration value="LongDashDot" />
			<xs:enumeration value="LongDashDotDot" />
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="queryParamType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Boolean" />
			<xs:enumeration value="Integer" />
			<xs:enumeration value="Float" />
			<xs:enumeration value="String" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="resetZoomType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="chart" />
			<xs:enumeration value="plot" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="minorTickType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="linear" />
			<xs:enumeration value="logarithmic" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="axisTitleAlign">
		<xs:restriction base="xs:string">
			<xs:enumeration value="low" />
			<xs:enumeration value="middle" />
			<xs:enumeration value="high" />
		</xs:restriction>
	</xs:simpleType>

</xs:schema>